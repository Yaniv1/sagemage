{
  "_DEFAULT_": {
    "name": "default_agent",
    "type": "GENERATIVE_ANALYZER",
    "about": "settings/about.txt",
    "project_dir": "./",
    "api_key": "settings/api_key.txt",
    "output_datasets": {
      "input_files": "input_files",
      "result_files": "result_files",
      "prompts": "prompts",
      "analysis": "analysis"
    },
    "output_version": "%Y-%m-%d-%H-%M-%S",
    "output_extension": "json",
    "json_node": "results",
    "outlets": [
      {
        "uri": "output.csv",
        "type": "FS"
      }
    ]
  },

  "a1_foofoo_classifier": {
    "inlets": [
      {
        "uri": "inputs/objects.csv",
        "type": "FS",
        "dataset_name": "foofoo",
        "dataset_id_column": "item_id",
        "dataset_columns": ["name"],
        "chunk_size": 2
      },
      {
        "uri": "inputs/types.csv",
        "type": "FS",
        "dataset_name": "types",
        "dataset_id_column": "tid",
        "dataset_columns": ["type_name"],
        "chunk_size": -1
      }
    ],
    "instructions": "instructions/classify.txt",
    "execute": true,
    "verbose": true,
    "outlets": [
      {
        "uri": "foofoo_classifier_results.csv",
        "type": "FS",
        "dataset_name": "foofoo",
        "result_columns": ["item_id", "name", "type"]
      },
      {
        "uri": "a1_foofoo_classifier",
        "type": "AM",
        "dataset_name": "foofoo",
        "result_columns": ["item_id", "name", "type"]
      }
    ]
  },

  "a2_foofoo_analyzer": {
    "name": "foofoo_analyzer",
    "inlets": [
      {
        "uri": "a1_foofoo_classifier",
        "type": "AM",
        "chunk_size": -1
      }
    ],
    "instructions": "instructions/analyze.txt",
    "execute": true,
    "verbose": true,
    "outlets": [
      {
        "uri": "foofoo_analysis_results.csv",
        "type": "FS",
        "dataset_name": "analysis",
        "result_columns": ["type", "items", "share"]
      }
    ]
  }
}
