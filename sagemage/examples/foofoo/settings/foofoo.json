{
  "_DEFAULT_": {
    "name": "default_agent",
    "type": "GENERATIVE_ANALYZER",
    "about": "settings/about.txt",
    "verbose": true,
    "delta": true,
    "project_dir": "./",
    "model":
      { "client": "openai.OpenAI", 
        "version": "gpt-5.1",    
        "temperature": 1,
        "max_tokens": 4096,
        "top_p": 1,
        "frequency_penalty": 0,
        "presence_penalty": 0
      },
    "api_key": "settings/api_key.txt",
    "textual": "ITEM",
    "output_datasets": {
      "input_files": "input_files",
      "result_files": "result_files",
      "prompts": "prompts",
      "analysis": "analysis"
    },

    "output_version": "%Y-%m-%d",
    "output_extension": "json",
    "json_node": "results",
    "outlets": [
      {
        "uri": "output.csv",
        "type": "FS"
      }
    ]
  },

  "a1_foofoo_classifier": {
    "name": "Classifier",
    "type": "GENERATIVE_ANALYZER",
    "about": "settings/about_classifier.txt",
    "execute": true,
    "verbose": true,
    "delta": true,
    "inlets": [
      {
        "uri": "inputs/objects.csv",
        "type": "FS",
        "dataset_name": "Objects",
        "dataset_id_column": "item_id",
        "dataset_columns": ["name"],        
        "chunk_size": 2,
        "chunks": 3,
        "sample": true
      },
      {
        "uri": "inputs/types.csv",
        "type": "FS",
        "dataset_name": "Types",
        "dataset_id_column": "tid",
        "dataset_columns": ["type_name"],
        "chunk_size": -1
      }
    ],
    "model": "gpt-5.1",
    "instructions": "instructions/classify.txt",    
    "result_columns": ["item_id", "name", "type"],
    "outlets": [
      {
        "uri": "foofoo_classifier_results",
        "type": "FS",
        "dataset_name": "Classifications",
        "dataset_id_column": "item_id",
        "result_columns": ["name", "type"]
      },
      {
        "uri": "a1_foofoo_classifier",
        "type": "AM",
        "dataset_name": "Classifications",
        "dataset_id_column": "item_id",
        "result_columns": ["name", "type"]
      }
    ]
  },

  "a2_foofoo_analyzer": {
    "name": "Analyzer",
    "type": "GENERATIVE_ANALYZER",
    "about": "settings/about_analyzer.txt",
    "execute": true,
    "verbose": true,
    "delta": true,
    "inlets": [
      {
        "uri": "a1_foofoo_classifier",
        "type": "AM",
        "dataset_name": "Classifications",
        "dataset_id_column": "item_id",
        "dataset_columns": ["name", "type"],
        "chunk_size": -1
      }
    ],
    "instructions": "instructions/analyze.txt",    
    "outlets": [
      {
        "uri": "a2_foofoo_analyzer_results",
        "type": "FS",
        "dataset_name": "Summary",
        "result_columns": ["type", "items", "share"]
      }
    ]
  },


  
  "a3_foofoo_create": {
    "name": "Creator",
    "type": "GENERATIVE_ANALYZER",
    "about": "settings/creator.txt",
    "execute": true,
    "verbose": true,
    "delta": false,
    "inlets": [
      {
        "uri": "a2_foofoo_analyzer",
        "type": "AM",
        "dataset_name": "Summary",
        "dataset_id_column": "type",
        "dataset_columns": ["items", "share"],
        "chunk_size": -1
      }
    ],
    "instructions": "instructions/create.txt",   
    "output_extension": "json", 
    "outlets": [
      {
        "uri": "a3_foofoo_create_results",
        "type": "FS",
        "dataset_name": "Story",
        "result_columns": ["story"]
      }
    ]
    
  }
}
